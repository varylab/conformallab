<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.varylab.com/conformallab/surface" 
	xmlns:surface="http://www.varylab.com/conformallab/surface" 
	xmlns:types="http://www.varylab.com/conformallab/types"
	elementFormDefault="qualified"
>
	<xs:import namespace="http://www.varylab.com/conformallab/types" schemaLocation="types.xsd"/>

	<xs:element name="Surface" type="surface:Surface"/>

	<xs:complexType name="Surface">
		<xs:choice>
			<xs:element name="SchottkyData" type="types:SchottkyData"/>
		</xs:choice>
	</xs:complexType>

</xs:schema>